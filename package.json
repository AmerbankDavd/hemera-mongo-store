{
  "name": "hemera-mongo-store",
  "version": "3.0.1",
  "description": "This is a plugin to use Mongodb with Hemera",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:hemerajs/hemera-mongo-store.git",
    "type": "git"
  },
  "scripts": {
    "lint": "eslint ./**/*.js",
    "test": "nyc mocha -t 5000 --exit ./test/**/*.js",
    "coverage": "npm run test && nyc report --reporter=html",
    "patch": "np patch --yolo",
    "minor": "np minor --yolo",
    "major": "np major --yolo"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "hemera"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "Dustin Deus (https://github.com/StarpTech)",
  "license": "MIT",
  "dependencies": {
    "hemera-plugin": "^0.1.1",
    "hemera-store": "^1.1.16",
    "mongodb": "2.2.x",
    "mongodb-extended-json": "1.10.x"
  },
  "devDependencies": {
    "code": "^3.0.2",
    "eslint": "^4.14.0",
    "eslint-config-hemera": "0.0.1",
    "hemera-joi": "^3.0.1",
    "hemera-testsuite": "^2.0.5",
    "mocha": "^4.0.1",
    "nats": "^0.7.24",
    "nats-hemera": "^3.1.3",
    "np": "^2.18.3",
    "nyc": "^11.4.1",
    "prettier": "^1.9.2"
  }
}
